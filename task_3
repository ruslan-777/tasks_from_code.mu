    <title>Таблица умножения</title>
</head>
<body>
    <script language="JavaScript"> 
        document.write("<h1><i>Таблица умножения</i></h1>"); 
         
        for (a = 1; a <= 9; a++){  
        document.write("<div style='float: left; width: 95px;'>"); 
        for (b = 1; b <=9; b++)  
        { 
        document.write(a + "*" + b + "=" +(a*b) + "<br><hr>"); 
         
        }  
        document.write ("</div>"); 
         
        } 
        </script> 
</body>
