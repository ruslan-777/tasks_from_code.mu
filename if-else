// console.log('yes');
// } else {
// console.log('no');
// }

// let test = 1;

// if (test != true) {
// console.log('yes');
// } else {
// console.log('no');
// }

// let test = NaN;

// if (test == true) {
// console.log('yes');
// } else {
// console.log('no');
// }

// let test = NaN;

// if (test == false) {
// console.log('yes');
// } else {
// console.log('no');
// }

// let test = '';

// if (test == false) {
// console.log('yes');
// } else {
// console.log('no');
// }

